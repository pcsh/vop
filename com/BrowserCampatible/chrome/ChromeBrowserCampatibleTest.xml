<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="账益达3.0浏览器兼容测试（谷歌）"  preserve-order="true"  verbose="2" >
   <test name="Test001_验证：登录页面，用户名和密码正确可以正常登录"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydLoginPageTestA" >
            <methods>
	            <include name="LoginPage"/>
            </methods>
            </class>
        </classes>
   </test>
   <test name="Test002_验证：业务管理-企业，页面能够正常打开"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydOpenCorpPageTest" >
            <methods>
	            <include name="OpenPage"/>
            </methods>
            </class>
        </classes>
   </test>
   <test name="Test003_验证：业务管理-企业主，新增企业主(AUTOADD01)"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydCorpOwnerPageTest" >
            <methods>
	            <include name="CorpOwnerPage"/>
            </methods>
            </class>
        </classes>
   </test>
   <test name="Test004_验证：业务管理-企业，新增企业(autoqy)"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydCorpAddTest" >
            <methods>
	            <include name="CorpAdd"/>
            </methods>
            </class>
        </classes>
   </test>
   <test name="Test005_验证：统计分析，九个页面都能正常打开"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydOpenStatisticsPageTest" >
            <methods>
	            <include name="OpenPage"/>
            </methods>
            </class>
        </classes>
    </test> 
   <test name="Test006_验证：业务管理-合同，新增合同(有合同的企业qy)"  preserve-order="true"   >
        <classes>
            <class name="com.BrowserCampatible.chrome.zydOrderAddTest" >
            <methods>
	            <include name="OrderAdd"/>
            </methods>
            </class>
        </classes>
   </test>
</suite>
